{
  "name": "react-widgets",
  "version": "1.0.0",
  "description": "react-widgets, 需结合react-dashboard-pro使用，依赖了antd, 使用了ts、less",
  "main": "index.js",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "deploy": "rm -rf dist && npm run build && node ./deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuanguandong/react-widgets.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuanguandong/react-widgets/issues"
  },
  "homepage": "https://github.com/yuanguandong/react-widgets#readme",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.20",
    "enquire-js": "^0.1.0",
    "prop-types": "^15.6.0",
    "rc-queue-anim": "^1.4.0",
    "rc-scroll-anim": "^2.0.2",
    "rc-tween-one": "^1.5.5",
    "react-document-title": "^2.0.3",
    "react-github-button": "^0.1.11"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0",
    "gh-pages": "^1.1.0"
  }
}
